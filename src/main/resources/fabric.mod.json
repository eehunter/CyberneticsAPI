{
  "schemaVersion": 1,
  "id": "cyberlib",
  "version": "${version}",

  "name": "Cybernetics API",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Ned Hunter"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/cyberlib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.oyosite.ticon.cyberlib.CyberLib"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.oyosite.ticon.cyberlib.CyberLibClient"
      }
    ]
  },
  "mixins": [
    "cyberlib.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": ">=1.19",
    "java": ">=16",
    "fabric-language-kotlin": ">=1.7.0+kotlin.1.6.0"
  },
  "suggests": {
  }
}
